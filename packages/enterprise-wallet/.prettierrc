{
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "printWidth": 140,
  "arrowParens": "avoid",
  "tabWidth": 4,
  "useTabs": true,
  "singleQuote": true,
  "trailingComma": "none",
  "semi": true,
  "bracketSpacing": true,
  "proseWrap": "always",
  "importOrder": [],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}